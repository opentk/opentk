                        <mml:mrow>
                            <mml:mi mathvariant="italic">v</mml:mi>
                            <mml:mo>&af;</mml:mo>
                            <mml:mfenced open="[" close="]">
                                <mml:mn>1</mml:mn>
                            </mml:mfenced>
                        </mml:mrow>
                        <mml:mrow>
                            <mml:mi mathvariant="italic">v</mml:mi>
                            <mml:mo>&af;</mml:mo>
                            <mml:mfenced open="[" close="]">
                                <mml:mn>2</mml:mn>
                            </mml:mfenced>
                        </mml:mrow>
                        <mml:mrow>
                            <mml:mi mathvariant="italic">v</mml:mi>
                            <mml:mo>&af;</mml:mo>
                            <mml:mfenced open="[" close="]">
                                <mml:mn>3</mml:mn>
                            </mml:mfenced>
                        </mml:mrow>
                    </mml:mfenced>
                </mml:mrow>
            </mml:math></inlineequation>,
