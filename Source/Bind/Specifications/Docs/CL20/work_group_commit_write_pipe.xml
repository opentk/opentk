<!-- ================================ SYNOPSIS -->

    <refnamediv id="work_group_commit_write_pipe">
        <refname>work_group_commit_write_pipe</refname>

        <refpurpose>
            Indicates that all writes to <varname>num_packets</varname> associated with a reservation are completed.
        </refpurpose>
    </refnamediv>

    <refsynopsisdiv xmlns:xlink="http://www.w3.org/1999/xlink"><title></title>
        <funcsynopsis>
            <funcprototype>
                <funcdef>
                    <link xlink:href="scalarDataTypes.html">void</link>
                    <function>
                        work_group_commit_write_pipe
                    </function>
                </funcdef>
                <paramdef>pipe gentype <parameter>p</parameter></paramdef>
                <paramdef><link xlink:href="enums.html#reserve_id_t">reserve_id_t</link><parameter>reserve_id</parameter></paramdef>
            </funcprototype>
        </funcsynopsis>

    </refsynopsisdiv>

<!-- ================================ DESCRIPTION  -->

    <refsect1 id="description"><title>Description</title>
        <para>
            Indicates that all writes to <varname>num_packets</varname>
            associated with reservation <varname>reserve_id</varname> are
            completed.
        </para>

        <para>
            This built-in function must be encountered by all work-items in 
            a work-group executing the kernel with the 
            same argument values; otherwise the behavior is 
            undefined.
        </para>

        &pipeFunctionsInc;

    </refsect1>

