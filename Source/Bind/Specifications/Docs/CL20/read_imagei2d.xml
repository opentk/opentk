<!-- ================================ SYNOPSIS -->

    <refnamediv id="read_image2d">
        <refname>read_image{i|ui} (2D)</refname>

        <refpurpose>
            Do an element lookup in the 2D image object, returning unnormalized signed integer and unsigned integer values.
        </refpurpose>
