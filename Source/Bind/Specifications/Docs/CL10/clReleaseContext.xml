<!-- ================================ SYNOPSIS -->  

    <refnamediv id="clGetDeviceIDs">
        <refname>             
            clReleaseContext
        </refname> 

        <refpurpose>          
            Decrement the context reference count.
        </refpurpose> 
    </refnamediv> 

    <refsynopsisdiv xmlns:xlink="http://www.w3.org/1999/xlink"><title></title>
        <funcsynopsis>
            <funcprototype>
                <funcdef>
<link xlink:href="scalarDataTypes.html">cl_int</link>
                    <function>
                        clReleaseContext
                    </function>
                </funcdef>

                <paramdef><link xlink:href="abstractDataTypes.html">cl_context</link><parameter>context</parameter></paramdef>   

            </funcprototype>
        </funcsynopsis>     
    </refsynopsisdiv>


<!-- ================================ PARAMETERS --> 

    <refsect1 id="parameters">
        <title>Parameters</title>
        <variablelist>

            <varlistentry>
                <term>
                    <varname> <!-- parameter name -->
                        context
                    </varname>
                </term>

                <listitem>
                    <para> <!-- parameter description -->
                        The context to release.
                    </para>
               </listitem>
            </varlistentry>
        </variablelist>
    </refsect1>

    
<!-- ================================ NOTES  -->        
 

    <refsect1 id="notes"><title>Notes</title>
        <para>
            After the <varname>context</varname> reference count becomes zero and all the objects attached to <varname>context</varname> (such as
memory objects, command-queues) are released, the <varname>context</varname> is deleted.
        </para>
    </refsect1>
    

<!-- ================================ ERRORS  -->       
 

    <refsect1 id="errors"><title>Errors</title>
        
		<para><anchor id="thisOne"/>
            Returns <errorname>CL_SUCCESS</errorname> if the function is executed successfully, or CL_INVALID_CONTEXT if context is not a valid OpenCL context.
        </para>
    </refsect1>
    

